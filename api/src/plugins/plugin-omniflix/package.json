{
  "name": "@elizaos-plugins/plugin-omniflix",
  "version": "0.25.6-alpha.1",
  "type": "module",
  "agentConfig": {
    "pluginType": "elizaos:client:1.0.0",
    "pluginParameters": {
      "OMNIFLIX_MNEMONIC": {
        "type": "string",
        "minLength": 1,
        "description": "Either mnemonic or private key (one is required)"
      },
      "OMNIFLIX_API_URL": {
        "type": "string",
        "minLength": 1,
        "default": "https://rest.omniflix.network",
        "description": "API endpoint for OmniFlix Network"
      },
      "OMNIFLIX_PRIVATE_KEY": {
        "type": "string",
        "minLength": 1,
        "description": "Either mnemonic or private key (one is required)"
      },
      "OMNIFLIX_RPC_ENDPOINT": {
        "type": "string",
        "minLength": 1,
        "default": "https://rpc.omniflix.network:443",
        "description": "RPC endpoint for OmniFlix Network"
      }
    }
  }
}
